{
  "__inputs": [],
  "__requires": [],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Prometheus internal metrics: TSDB, Scrapes, Rules, Targets, Queries",
  "editable": true,
  "gnetId": 3662,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "description": "Number of active Prometheus targets",
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "expr": "sum(prometheus_target_scrape_pool_targets)",
          "refId": "A"
        }
      ],
      "title": "Total Targets",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "Number of failed scrapes",
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "sum(increase(prometheus_target_scrapes_exceeded_body_size_total[5m]))",
          "refId": "A"
        }
      ],
      "title": "Failed Scrapes (5m)",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "Scrape duration over time",
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(prometheus_target_interval_length_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "title": "Scrape Duration (95th percentile)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "Rule evaluation duration",
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "id": 4,
      "targets": [
        {
          "expr": "sum by(rule_group) (prometheus_rule_group_last_duration_seconds)",
          "refId": "A"
        }
      ],
      "title": "Rule Evaluation Duration",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "Prometheus query performance",
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 26
      },
      "id": 5,
      "targets": [
        {
          "expr": "rate(prometheus_engine_query_duration_seconds_sum[5m]) / rate(prometheus_engine_query_duration_seconds_count[5m])",
          "refId": "A"
        }
      ],
      "title": "Query Duration (Avg 5m)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "description": "TSDB head chunks",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 35
      },
      "id": 6,
      "targets": [
        {
          "expr": "prometheus_tsdb_head_chunks",
          "refId": "A"
        }
      ],
      "title": "TSDB Head Chunks",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "Storage write throughput",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 35
      },
      "id": 7,
      "targets": [
        {
          "expr": "rate(prometheus_tsdb_compaction_chunk_samples_sum[5m])",
          "refId": "A"
        }
      ],
      "title": "TSDB Sample Rate (5m)",
      "type": "stat"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["prometheus", "monitoring"],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timezone": "",
  "title": "Prometheus Overview Dashboard",
  "uid": "prom-overview",
  "version": 1
}

