[prometheus]
# replace PROM_PRIVATE_IP and BASTION_PUBLIC_IP with outputs or let Jenkins fill them
PROM_PRIVATE_IP ansible_user=ubuntu ansible_ssh_private_key_file=/home/uday/new-uday-key.pem

[all:vars]
ansible_ssh_common_args='-o ProxyCommand="ssh -W %h:%p -i /home/uday/new-uday-key.pem ubuntu@BASTION_PUBLIC_IP"'

